<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="account_move_invoice" model="ir.sequence">
            <field name="name">Invoice</field>
            <field name="code">account.move.invoice</field>
            <field name="prefix">INV/%(year)s/%(month)s/</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="account_move_vendor_bill" model="ir.sequence">
            <field name="name">Invoice</field>
            <field name="code">account.move.vendor.bill</field>
            <field name="prefix">BILL/%(year)s/%(month)s/</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>